{
  "name": "vue3-element-admin",
  "private": true,
  "author": "yulinZ",
  "version": "1.0.0",
  "license": "ISC",
  "***": "electron 入口文件",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vite build --mode prod",
    "preview": "vite preview --mode prod",
    "electron": "wait-on tcp:3000 && cross-env NODE_ENV=development electron .",
    "electron:serve": "concurrently -k \"pnpm dev\" \"pnpm electron\"",
    "electron:start": "electron .",
    "electron:generate-icons": "electron-icon-builder --input=./src/assets/logo.png --output=dist_electron/build --flatten",
    "electron:build-win": "pnpm electron:generate-icons && vite build && electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vueuse/core": "^9.3.0",
    "axios": "^1.1.3",
    "echarts-wordcloud": "^2.0.0",
    "element-plus": "^2.2.18",
    "file-saver": "^2.0.5",
    "github-markdown-css": "^5.1.0",
    "ityped": "^1.0.3",
    "lodash": "^4.17.21",
    "md-editor-v3": "^2.3.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.14",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.1.3",
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/logos": "^1.1.16",
    "@iconify-json/mdi": "^1.1.34",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "echarts": "^5.4.0",
    "electron": "^21.1.1",
    "electron-builder": "^23.3.3",
    "electron-icon-builder": "^2.0.1",
    "happy-dom": "^7.5.12",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.18",
    "sass": "^1.55.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.12",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vue-tsc": "^0.40.4"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright © 2022 yulinZ",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "dist_electron/build/icons/icon.ico",
      "target": [
        "nsis",
        "msi"
      ]
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}
